# Environment Variables Template
# Copy this file to .env.local and fill in the values

# API Configuration
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Work Directory (absolute path or relative to project root)
WORK_DIR=./claude-agents-output

# Security Settings
NODE_ENV=development

# Allowed domains for URL testing (comma-separated)
ALLOWED_DOMAINS=baidu.com,google.com,localhost

# Logging Level
LOG_LEVEL=info

# Performance Settings
MAX_CONCURRENT_REQUESTS=5
REQUEST_TIMEOUT=30000

# Development Settings (only in development)
ENABLE_DEBUG_LOGS=true
DISABLE_AUTH=false